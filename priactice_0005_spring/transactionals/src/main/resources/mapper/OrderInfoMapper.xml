<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tool.zgd.base.dao.structure.OrderInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.transactional.entity.OrderInfo">
        <id column="id" property="id" />
        <result column="buy_name" property="buyName" />
        <result column="buy_goods" property="buyGoods" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, buy_name AS buyName, buy_goods AS buyGoods, create_time AS createTime, update_time AS updateTime
    </sql>

</mapper>
